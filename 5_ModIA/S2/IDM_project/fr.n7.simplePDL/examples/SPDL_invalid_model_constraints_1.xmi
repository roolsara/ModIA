<?xml version="1.0" encoding="UTF-8"?>
<simplepdl:Process
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:simplepdl="http://simplepdl"
    name="tired_process">
  <processElements
      xsi:type="simplepdl:Guidance"
      elements="//@processElements.2"
      text="I think that WD2 is too much hungry..."/>
  <processElements
      xsi:type="simplepdl:WorkSequence"
      predecessor="//@processElements.2"
      successor="//@processElements.2"/>
  <processElements
      xsi:type="simplepdl:WorkDefinition"
      linksToPredecessors="//@processElements.1"
      linksToSuccessors="//@processElements.1"
      name="4WD1">
    <useressource
        ressource="//@processElements.3"
        quantity="2"/>
  </processElements>
  <processElements
      xsi:type="simplepdl:Ressource"
      name="R1"
      useressource="//@processElements.2/@useressource.0 //@processElements.4/@useressource.0"/>
  <processElements
      xsi:type="simplepdl:WorkDefinition"
      name="WD2">
    <useressource
        ressource="//@processElements.3"
        quantity="10"/>
  </processElements>
</simplepdl:Process>
